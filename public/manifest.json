{
  "manifest_version": 3,
  "name": "Chrome Clipboard - Frequently used text manager",
  "version": "1.0",
  "description": "Save your frequently used info and never type them again.",

  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "permissions": ["storage"]
}
